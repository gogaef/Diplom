@model IEnumerable<StartupArticle>
@{

}
<h2>Admin Panel</h2>

<div>
    <ul>
        @foreach(var startup in Model)
        {
            <li>
                @startup.Title
                -
                <a asp-controller="Panel" asp-action="Edit" asp-route-id="@startup.Id">
                    Edit
                </a>
                -
                <a asp-controller="Panel" asp-action="Remove" asp-route-id="@startup.Id">
                    Remove
                </a>
            </li>
        }
    </ul>
</div>